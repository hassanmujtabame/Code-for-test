{
  "name": "riadiat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-minasa": "npm run build -- --env.PREFIX_URL=minasa",
    "serve-minasa": "cross-env PREFIX_URL=minasa vue-cli-service serve",
    "prod": "vue-cli-service build --max_old_space_size=4096",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckeditor/ckeditor5-vue2": "^3.0.1",
    "aos": "^2.3.1",
    "bootstrap": "^5.3.0",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.8.3",
    "dayjs": "^1.11.9",
    "imask": "^6.4.3",
    "jquery": "^3.6.1",
    "js-cookie": "^3.0.1",
    "portal-vue": "^2.1.7",
    "sweetalert2": "^11.7.0",
    "swiper": "^6.8.4",
    "swiper-vue2": "^1.0.10",
    "uuid": "^9.0.0",
    "v-calendar": "^2.4.1",
    "v-owl-carousel": "^1.0.8",
    "v-tooltip": "^2.1.3",
    "vee-validate": "^3.3.3",
    "vue": "^2.7.14",
    "vue-aos": "^3.1.0",
    "vue-audio-visual": "^2.5.0",
    "vue-i18n": "^8.26.3",
    "vue-mention": "^1.1.0",
    "vue-meta": "^2.4.0",
    "vue-multiselect": "^2.1.6",
    "vue-qrcode-component": "^2.1.1",
    "vue-router": "^3.5.1",
    "vue-social-sharing": "^3.0.9",
    "vue2-google-maps": "^0.10.7",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-vuex": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "axios": "^0.18.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-vue": "^8.0.3",
    "laravel-echo": "^1.15.0",
    "pusher-js": "^8.0.1",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-vue-i18n": "~1.0.1",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-undef": "warn",
      "no-useless-escape": "warn",
      "no-cond-assign": "warn",
      "vue/no-unused-components": "warn"
    }
  },
  "browserslist": "defaults"
}
